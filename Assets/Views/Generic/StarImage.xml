<StarImage xmlns="MarkLight"
           Width="11ux" Height="10ux">
  <ViewSwitcher Id="StarSelector"
                StartView="EmptyStar"
                SwitchToDefault="True">
    <Image Id="EmptyStar" Sprite="{@Images.Star}:Empty" />
    <Image Id="FilledStar" Sprite="{@Images.Star}:Filled" />
  </ViewSwitcher>
</StarImage>